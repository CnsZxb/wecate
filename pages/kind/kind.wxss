/* pages/kind/kind.wxss */

.discription {
  background: #fff;
  height: 2.55rem;
  width: 100%;
  font-size: 0.8125rem;
  position: fixed;
  top: 0;
  z-index: 999999;
}

.discription .disBtn {
  height: 2.55rem;
  width: 100%;
  display: flex;
  justify-content: space-around;
  line-height: 2.55rem;
  color: #666;
}

.discription .disBtn .startStyle {
  color: rgba(212, 166, 87, 1);
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-position: bottom;
}

.goods {
  height: 7.375rem;
  z-index: 1;
  margin-top: 2.25rem;
  position: relative;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}

.shade1, .shade2, .shade3 {
  width: 100%;
  height: 7.375rem;
  background: rgba(167, 15, 15, 0.7);
  color: #fff;
  position: absolute;
  top: 0;
  left: 0;
  text-align: center;
}

.shade2 {
  background: rgba(12, 118, 141, 0.65);
}

.shade3 {
  background: rgba(97, 97, 97, 0.7);
}

.shade_title {
  height: 2.5625rem;
  font-weight: 600;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 0.8125rem;
  padding-top: 0.3rem;
}

.shade_title::before {
  content: "";
  display: inline-block;
  width: 2rem;
  height: 1px;
  background-color: #fff;
  margin-right: 0.6rem;
}

.shade_title::after {
  content: "";
  display: inline-block;
  width: 2rem;
  height: 1px;
  background-color: #fff;
  margin-left: 0.6rem;
}

.describe {
  height: 1rem;
  font-size: 0.6875rem;
  font-weight: 500;
  letter-spacing: 0.2rem;
  padding-top: 0;
}

page {
  background: #f6f6f6;
}

.huangjin {
  width: 95%;
  margin: 0 auto;
  padding: 0 0.75rem;
  overflow: hidden;
  box-sizing: border-box;
  background-color: #fff;
  z-index: 22;
  position: relative;
  top: -1.6rem;
}

.huangjin .huangjin_list {
  width: 50%;
  height: 16.65rem;
  float: left;
  position: relative;
  box-sizing: border-box;
  overflow: hidden;
  text-align: center;
  line-height: 2;
  margin-bottom: 1rem;
  font-size: 0.6875rem;
}

.img_box2 {
  width: 100%;
  height: 10rem;
  margin-bottom: 0.6rem;
}

.img_box2 image {
  width: 100%;
  height: 10.375rem;
}

.now_buy {
  width: 3.9375rem;
  height: 1.375rem;
  border: 1px solid #e1e1e1;
  color: #fff;
  background: #333;
  font-size: 0.6875rem;
  line-height: 1.375rem;
  margin: 0 auto;
}

.goods_title {
  width: 70%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 0 auto;
}

.yushou {
  margin-top: -0.7rem;
}

.presale_item2 {
  padding: 0.9375rem;
  padding-bottom: 0.9375rem;
  border-bottom: 1px solid #e1e1e1;
}

.presale_item2 navigator {
  display: flex;
  align-items: center;
}

.img_box3 {
  width: 5rem;
  height: 5rem;
  display: flex;
  justify-content: center;
}

.img_box3 image {
  width: 100%;
  height: 100%;
}

.item_list {
  position: relative;
  left: 0.6rem;
  line-height: 1.7;
}

.item_title {
  color: #333;
  font-weight: 500;
  font-size: 0.6875rem;
}

.item_time {
  background-color: #418897;
  color: #fff;
  font-size: 0.6875rem;
  padding: 0.0625rem 0.3125rem;
}

.item_qian, .item_man {
  color: #333;
  font-size: 0.6875rem;
  white-space: nowrap;
}

.price {
  color: #d0021b;
  font-weight: 600;
}

.hot_buy {
  width: 95%;
  margin: 0 auto;
  overflow: hidden;
  box-sizing: border-box;
  background-color: #fff;
  z-index: 22;
  position: relative;
  top: -1.6rem;
  margin-bottom: 0.5rem;
}

.buy_list {
  margin: 0 auto;
  background-color: #fff;
  border-radius: 0.25rem;
  padding: 0.6rem;
  border-bottom: 0.6rem solid #f6f6f6;
}

.buy_list>navigator {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.img_box {
  width: 6rem;
  height: 6rem;
  position: relative;
}

.img_box image {
  width: 100%;
  height: 100%;
}

.buy_details {
  width: 30%;
  font-size: 0.8125rem;
  position: relative;
  flex: 6.5;
  margin-left: 0.9375rem;
}

.buy_name {
  font-size: 0.6875rem;
  font-weight: 500;
}

.buy_bao {
  display: inline-block;
  color: #d0021b;
  font-size: 0.6875rem;
  border: 1px solid #d0021b;
  border-radius: 0.25rem;
  margin: 0.5375rem 0 1rem 0;
}

.buy_old {
  margin-bottom: 0.25rem;
  color: #737373;
  font-size: 0.6875rem;
}

.del {
  text-decoration: line-through;
}

.buy_qiang {
  font-size: 0.6875rem;
}

.now_hot {
  width: 3.5rem;
  height: 1.5625rem;
  background: linear-gradient(360deg, rgba(239, 71, 57, 1) 0%, rgba(243, 106, 73, 1) 100%);
  border-radius: 0.1875rem;
  border: none;
  position: absolute;
  text-align: center;
  line-height: 1.5625rem;
  font-size: 0.7rem;
  bottom: 0;
  right: 0;
  color: #fff;
}

.buy_list:nth-child(1) .hot_one {
  width: 1.46875rem;
  height: 1.65625rem;
  position: absolute;
  top: 0;
  left: 0;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}

.buy_list:nth-child(2) .hot_one {
  width: 1.46875rem;
  height: 1.65625rem;
  position: absolute;
  top: 0;
  left: 0;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}

.buy_list:nth-child(3) .hot_one {
  width: 1.46875rem;
  height: 1.65625rem;
  position: absolute;
  top: 0;
  left: 0;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
}
